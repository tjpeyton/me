{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "no-console": "warn",
    "prefer-const": "error",
    "no-useless-return": "error",
    "no-useless-catch": "error",
    "no-unneeded-ternary": "error",
    "no-unsafe-finally": "error"
  }
}
